<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ecom.khawadawa_main">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Khawadawa_main">
        <activity android:name=".ShowProductsUser"></activity>
        <activity android:name=".ShowProductVendor" />
        <activity android:name=".AddProductVendor" />
        <activity android:name=".VendorSignUp" />
        <activity android:name=".VendorDashboard" />
        <activity android:name=".VendorSignIn" />
        <activity android:name=".Profile" />
        <activity android:name=".Cart" />
        <activity android:name=".Orders" />
        <activity android:name=".SignIn" />
        <activity android:name=".Dashboard" />
        <activity android:name=".signup" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>